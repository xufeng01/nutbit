<script type="text/javascript" src='js/lib/EasyUI1.3/jquery.easyui.min.js'></script>
<script type="text/javascript" src='js/lib/EasyUI1.3/locale/easyui-lang-zh_CN.js'></script>
<script type="text/javascript" src='js/lib/EasyUI1.3/util.js'></script>
</head>
<frameset rows="88,*" cols="*" frameborder="no" border="0" framespacing="0">
