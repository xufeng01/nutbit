<%-- 下拉--%>
<script type="text/javascript" src="<%=path %>/js/select_tree.js"></script>
<script type="text/javascript" src="<%=path %>/js/select_option.js"></script>

<%-- 文件上传--%>
<script type="text/javascript" src="<%=path%>/js/lib/ajaxfileupload.js"></script>

<%-- 获得frame--%>
<script type="text/javascript" src="<%=path%>/js/util.js"></script>

<%-- flash--%>
<script type="text/javascript" src="<%=path%>/js/lib/swfobject.js"></script>

<%-- jquery验证--%>
<script type="text/javascript" src="<%=path%>/js/lib/validate/jquery.validate.js"></script>
